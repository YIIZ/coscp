{
  "name": "@teambun/coscp",
  "version": "3.1.1",
  "description": "Efficient file transfer for QCloud COS.",
  "repository": "yiiz/coscp",
  "bin": "src/cli.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "code-quality": "npm run lint && npm run test"
  },
  "files": [
    "src"
  ],
  "main": "src/index.js",
  "keywords": [
    "cli",
    "qcloud"
  ],
  "dependencies": {
    "caporal": "^0.10.0",
    "cli-table": "^0.3.1",
    "cos-nodejs-sdk-v5": "https://github.com/tencentyun/cos-nodejs-sdk-v5.git#298b2a",
    "cosmiconfig": "^5.0.5",
    "debug": "^3.1.0",
    "draftlog": "^1.0.12",
    "fast-glob": "^2.2.2",
    "md5-file": "^4.0.0",
    "p-map": "^1.2.0",
    "run-function": "^0.1.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "prettier --write",
        "eslint",
        "git add"
      ],
      "**/*.{json,md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "license": "MIT"
}
